stages:
  Get Instance Fields:
    actors:
      Get Table Instance Fields:
        parent: InnerFlow
        in:
          flowName:
            const: GetTableInstanceFields
          ref_instance_id:
            external: ref_instance_id
            schema: string
            mandatory: false
        out:
          result:
            schema: '#ref'
      getTDMDBSchema:
        parent: InnerFlow
        in:
          flowName:
            const: getTDMDBSchema
        out:
          schema:
            schema: string
  Get Task Execution Id:
    actors:
      Task Execution ID:
        parent: JavaScript
        in:
          script:
            const: |-
              var taskExecutionId = taskExeId;
              if (taskExeId == null) {
                 taskExecutionId = idFromInstance;
              }
              taskExecutionId;
          taskExeId:
            external: taskExeId
            schema: integer
            mandatory: false
          idFromInstance:
            link: Get Table Instance Fields/result/task_execution_id
            schema: integer
            mandatory: false
        out:
          result:
            schema: integer
  Get Details:
    actors:
      Get Table Details:
        parent: DbFetchFirstRow
        in:
          interface:
            const: TDM
          sql:
            const: |
              select rt.lu_name,rt.task_ref_table_id as ref_table_id, rt.ref_table_name, ${source_env} as source_env, t.task_type,t.selection_method,
                  rt.interface_name, rt.schema_name, es.task_execution_id, rt.task_id, rt.filter_type, COALESCE(rt.table_filter, '') as table_filter,
                  COALESCE(rt.gui_filter, '') as gui_filter,
                  COALESCE(rt.filter_parameters, '') as filter_parameters, rt.filter_fields, t.retention_period_type, t.retention_period_value,
                  rt.interface_name as target_interface,
                  rt.schema_name as target_schema,
                  coalesce(rt.target_table_prefix,'')|| rt.ref_table_name || coalesce(rt.target_table_suffix,'') as target_table_name,
                  coalesce(es.number_of_records_to_process, -1) as number_of_records_to_process,
                  CASE
                      WHEN t.task_type = 'LOAD' AND rt.version_task_execution_id IS NOT NULL
                      THEN rt.version_task_execution_id
                      ELSE 0
                  END AS version_task_execution_id
              from ${@TDMDB_SCHEMA}.task_ref_tables rt, ${@TDMDB_SCHEMA}.task_ref_exe_stats es, ${@TDMDB_SCHEMA}.tasks t
              where es.task_execution_id = ${task_execution_id}
              and es.task_id = rt.task_id
              and es.task_ref_table_id = rt.task_ref_table_id
              and rt.ref_table_name = ${ref_table_name}
              and rt.interface_name = ${interface_name}
              and rt.schema_name = ${schema_name}
              and rt.task_id = t.task_id
          source_env:
            link: Get Table Instance Fields/result/source_env
            schema: string
            mandatory: false
          task_execution_id:
            link: Task Execution ID/result
            schema: integer
            mandatory: false
          ref_table_name:
            link: Get Table Instance Fields/result/table_name
            schema: string
            mandatory: false
          interface_name:
            link: Get Table Instance Fields/result/interface_name
            schema: string
            mandatory: false
          schema_name:
            link: Get Table Instance Fields/result/schema_name
            schema: string
            mandatory: false
          TDMDB_SCHEMA:
            link: getTDMDBSchema/schema
            schema: string
            mandatory: false
        out:
          result:
            schema: '#ref'
  Get Target Details:
    actors:
      Get Target Details By Source:
        parent: InnerFlow
        in:
          flowName:
            const: GetTargetDetailsBySource
          luName:
            link: Get Table Details/result/lu_name
            schema: string
            editor:
              id: com.k2view.mTableKey
            mandatory: false
          interfaceName:
            link: Get Table Details/result/interface_name
            schema: string
            editor:
              id: com.k2view.mTableKey
            mandatory: false
          schemaName:
            link: Get Table Details/result/schema_name
            schema: string
            editor:
              id: com.k2view.mTableKey
            mandatory: false
          tableName:
            link: Get Table Details/result/ref_table_name
            schema: string
            editor:
              id: com.k2view.mTableKey
            mandatory: false
        out:
          targetInterface:
            schema: string
          targetSchema:
            schema: string
          targetTableName:
            schema: string
          truncate_indicator:
            schema: string
          count_indicator:
            schema: string
          count_flow:
            schema: string
          sourceSchemaName:
            schema: string
  Create Target Table Details:
    actors:
      Parse Filter Inputs:
        parent: JsonParser
        in:
          stream:
            link: Get Table Details/result/filter_fields
        out:
          object:
            schema: '#ref'
      Create Map for Target Interface:
        parent: MapBuild
        in:
          key:
            const: target_interface
            schema: string
          value:
            link: Get Target Details By Source/targetInterface
            schema: string
        out:
          map:
            schema: '#ref'
      Create Map for Target Table Name:
        parent: MapBuild
        in:
          key:
            const: target_table_name
            schema: string
          value:
            link: Get Target Details By Source/targetTableName
            schema: string
        out:
          map:
            schema: '#ref'
      GetSchemaForCatalog1:
        parent: InnerFlow
        in:
          flowName:
            const: GetSchemaForCatalog
          interface:
            link: Get Table Details/result/interface_name
            schema: string
            editor:
              id: com.k2view.mTableKey
            mandatory: false
          schema:
            link: Get Target Details By Source/sourceSchemaName
            schema: string
            editor:
              id: com.k2view.mTableKey
            mandatory: false
          table:
            link: Get Table Details/result/ref_table_name
            schema: string
            editor:
              id: com.k2view.mTableKey
            mandatory: false
          taskSchema:
            link: Get Table Details/result/schema_name
            schema: string
            mandatory: false
        out:
          result:
            schema: string
  Get Retention:
    actors:
      Set TARGET_ENTITY_ID:
        parent: FabricSet
        in:
          key:
            const: TARGET_ENTITY_ID
            default: false
          value:
            link: Get Target Details By Source/targetTableName
      Calculate Retention:
        parent: LuFunction
        in:
          functionName:
            const: getRetention
          retentionPeriodType:
            link: Get Table Details/result/retention_period_type
            schema: string
            mandatory: false
          retentionPeriodValue:
            link: Get Table Details/result/retention_period_value
            schema: integer
            mandatory: false
        out:
          result:
            schema: integer
      Check if There is a Schema:
        parent: MTableLookup
        in:
          mtable:
            const: TableLevelInterfaces
          interface_name:
            link: Get Table Details/result/interface_name
            schema: string
            editor:
              id: com.k2view.mTableKey
            mandatory: false
        out:
          result:
            schema: '#ref'
          no_schema:
            schema: string
      Create Map for Catalog Schema:
        parent: MapBuild
        in:
          key:
            const: catalog_schema_name
            schema: string
          value:
            link: GetSchemaForCatalog1/result
            schema: string
        out:
          map:
            schema: '#ref'
      Create Map for Catalog Schema1:
        parent: MapBuild
        in:
          key:
            const: version_execution_id
            schema: string
          value:
            link: Get Table Details/result/version_task_execution_id
            schema: string
        out:
          map:
            schema: '#ref'
  Check Schema:
    dependsOn: Get Retention
    actors:
      There is No Schema?:
        parent: Equals
        condition: result
        in:
          a:
            link: Check if There is a Schema/no_schema
            schema: string
          b:
            const: 'true'
            schema: string
      Create Map for Source Schema With Empty Value:
        parent: MapBuild
        in:
          key:
            const: schema_name
            schema: string
          value:
            const: ''
            schema: string
        out:
          map:
            schema: '#ref'
      Create Map for Target Schema with Empty Value:
        parent: MapBuild
        in:
          key:
            const: target_schema
            schema: string
          value:
            const: ''
            schema: string
        out:
          map:
            schema: '#ref'
    split: '--------------------'
  Stage 4:
    else: true
    dependsOn: Get Retention
    actors:
      Create Map for Source Schema:
        parent: MapBuild
        in:
          key:
            const: schema_name
            schema: string
          value:
            link: Get Target Details By Source/sourceSchemaName
            schema: string
        out:
          map:
            schema: '#ref'
      Create Map for Target Scherna:
        parent: MapBuild
        in:
          key:
            const: target_schema
            schema: string
          value:
            link: Get Target Details By Source/targetSchema
            schema: string
        out:
          map:
            schema: '#ref'
  Create Retention Map:
    actors:
      Create Map for Retention:
        parent: MapBuild
        in:
          key:
            const: retention
            schema: string
          value:
            link: Calculate Retention/result
            schema: integer
        out:
          map:
            schema: '#ref'
      Create Map for  Filter Fields Info:
        parent: MapBuild
        in:
          key:
            const: filter_fields
            schema: string
          value:
            link: Parse Filter Inputs/object
            schema: integer
        out:
          map:
            schema: '#ref'
  Merge:
    actors:
      Marge Retention to Output:
        parent: MapMerge
        in:
          maps:
            link:
            - path: Get Table Details/result
              pos: 0
            - path: Create Map for Retention/map
              pos: 1
            - path: Create Map for Target Table Name/map
              pos: 2
            - path: Create Map for Target Interface/map
              pos: 3
            - path: Create Map for Source Schema With Empty Value/map
              pos: 4
            - path: Create Map for Target Schema with Empty Value/map
              pos: 5
            - path: Create Map for Target Scherna/map
              pos: 5
            - path: Create Map for  Filter Fields Info/map
              pos: 6
            - path: Create Map for Source Schema/map
              pos: 7
            - path: Create Map for Catalog Schema/map
              pos: 8
            - path: Create Map for Catalog Schema1/map
              pos: 9
        out:
          map:
            external: result
            schema: '#ref'
schemas:
  Get Table Instance Fields.out.result:
    type: object
    properties:
      source_env:
        type: string
      interface_name:
        type: string
      schema_name:
        type: string
      table_name:
        type: string
      task_execution_id:
        type: string
  Get Table Details.out.result:
    type: object
