stages:
  Input:
    actors:
      SyncDeleteMode:
        parent: SyncDeleteMode
        in:
          interface:
            schema: any
          table:
            const: null
            external: table
      getTDMDBSchema:
        parent: InnerFlow
        in:
          flowName:
            const: getTDMDBSchema
        out:
          schema:
            schema: string
      PopulationArgs:
        parent: PopulationArgs
        readonly: true
        in:
          task_execution_id:
            external: task_execution_id
            schema: any
            mandatory: false
        out:
          parent_rows:
            schema: '#ref'
  Source:
    actors:
      Query:
        parent: SourceDbQuery
        in:
          interface:
            const: TDM
          sql:
            const: Select * From ${@TDMDB_SCHEMA}.task_exe_stats_detailed
          TDMDB_SCHEMA:
            link: getTDMDBSchema/schema
            schema: string
            mandatory: false
          parent_rows:
            link: PopulationArgs/parent_rows
        out:
          result:
            schema: '#ref'
  Get Entity Status:
    actors:
      DbFetchField:
        parent: DbFetchField
        in:
          interface:
            const: fabric
          sql:
            const: select EXECUTION_STATUS from TDM.TASK_EXECUTION_LINK_ENTITIES where
              LU_NAME = ${LU_NAME} and TARGET_ENTITY_ID = ${TARGET_ENTITY_ID} and
              IID=${IID}
          IID:
            link:
              path: Query/result/entity_id
              iterate: Iterate
            schema: any
            mandatory: false
          TARGET_ENTITY_ID:
            link:
              path: Query/result/target_entity_id
              iterate: Iterate
            schema: any
            mandatory: false
          LU_NAME:
            link:
              path: Query/result/lu_name
              iterate: Iterate
            schema: any
            mandatory: false
  'Succussed? ':
    actors:
      EqualsIgnoreCase:
        parent: EqualsIgnoreCase
        condition: result
        in:
          b:
            const: completed
            schema: string
          a:
            link: DbFetchField/result
  LU Table:
    last: 1
    actors:
      task_exe_stats_detailed:
        parent: DbLoad
        in:
          interface:
            const: fabric
          schema:
            const: TDM
          table:
            const: TASK_EXE_STATS_DETAILED
          fields:
            const:
            - task_execution_id
            - lu_name
            - entity_id
            - target_entity_id
            - table_name
            - stage_name
            - flow_name
            - actor_name
            - creation_date
            - source_count
            - target_count
            - diff
            - results
            - suppressed_error_count
          keys:
            const:
            - TASK_EXECUTION_ID
            - LU_NAME
            - TARGET_ENTITY_ID
            - TABLE_NAME
          dialect:
            const: sqlite
          TASK_EXECUTION_ID:
            schema: any
            mandatory: false
          LU_NAME:
            schema: any
            mandatory: false
          ENTITY_ID:
            schema: any
            mandatory: false
          TARGET_ENTITY_ID:
            schema: any
            mandatory: false
          TABLE_NAME:
            schema: any
            mandatory: false
          STAGE_NAME:
            schema: any
            mandatory: false
          FLOW_NAME:
            schema: any
            mandatory: false
          ACTOR_NAME:
            schema: any
            mandatory: false
          CREATION_DATE:
            schema: any
            mandatory: false
          SOURCE_COUNT:
            schema: any
            mandatory: false
          TARGET_COUNT:
            schema: any
            mandatory: false
          DIFF:
            schema: any
            mandatory: false
          RESULTS:
            schema: any
            mandatory: false
          suppressed_error_count:
            schema: any
            mandatory: false
          params:
            link:
              path: Query/result
              iterate: Iterate
  Post Load: {
    }
schemas:
  PopulationArgs.out.parent_rows:
    type: array
    items:
      type: object
      properties:
        task_execution_id:
          type: string
  Query.out.result:
    type: array
    items:
      type: object
      properties:
        task_execution_id:
          type: integer
        lu_name:
          type: string
        entity_id:
          type: string
        target_entity_id:
          type: string
        table_name:
          type: string
        stage_name:
          type: string
        flow_name:
          type: string
        actor_name:
          type: string
        creation_date:
          type: date
        source_count:
          type: string
        target_count:
          type: string
        diff:
          type: string
        results:
          type: string
        suppressed_error_count:
          type: string
