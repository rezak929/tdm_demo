stages:
  Input:
    actors:
      SyncDeleteMode:
        parent: SyncDeleteMode
        in:
          interface:
            schema: any
          table:
            const: null
            external: table
      PopulationArgs:
        parent: PopulationArgs
        readonly: true
        in:
          TASK_EXECUTION_ID:
            external: TASK_EXECUTION_ID
            schema: any
            mandatory: false
        out:
          parent_rows:
            schema: '#ref'
  Get Globals:
    actors:
      POP_FULL_LU_HIERARCHY_IN_TDM_LU:
        parent: FabricSetRead
        in:
          key:
            const: POP_FULL_LU_HIERARCHY_IN_TDM_LU
            default: false
      getTDMDBSchema:
        parent: InnerFlow
        in:
          flowName:
            const: getTDMDBSchema
        out:
          schema:
            schema: string
  'True':
    dependsOn: Get Globals
    actors:
      EqualsIgnoreCase:
        parent: EqualsIgnoreCase
        condition: result
        in:
          b:
            const: 'true'
            schema: string
          a:
            link: POP_FULL_LU_HIERARCHY_IN_TDM_LU/result
    split: '--------------------'
  'False':
    else: true
    dependsOn: Get Globals
  Select Query 1:
    dependsOn: 'True'
    actors:
      Query All Records:
        parent: DbCommand
        in:
          interface:
            const: TDM
          sql:
            const: |
              SELECT
                  task_execution_id,
                  lu_name,
                  entity_id,
                  target_entity_id,
                  execution_status,
                  iid,
                  id_type,
                  version_task_execution_id,
                  subset_task_execution_id,
                  parent_lu_name,
                  CASE
                      WHEN parent_entity_id IS NULL THEN ''
                      ELSE parent_entity_id
                  END AS parent_entity_id,
                  parent_target_entity_id,
                  root_entity_id,
                  root_target_entity_id,
                  root_lu_name,
                  execution_note
              FROM
                  ${@TDMDB_SCHEMA}.task_execution_entities
              WHERE
                  task_execution_id = ${task_execution_id};
          TDMDB_SCHEMA:
            link: getTDMDBSchema/schema
            schema: string
            mandatory: false
          task_execution_id:
            link:
              path: PopulationArgs/parent_rows/TASK_EXECUTION_ID
              iterate: First
            schema: '#ref'
            mandatory: false
        out:
          result:
            schema: '#ref'
    split: '--------------------'
  Select Query 2:
    dependsOn: 'False'
    actors:
      Query With Limited Records:
        parent: DbCommand
        in:
          interface:
            const: TDM
          sql:
            const: |-
              SELECT
                  t.task_execution_id,
                  t.lu_name,
                  t.entity_id,
                  t.target_entity_id,
                  t.execution_status,
                  t.iid,
                  t.id_type,
                  t.version_task_execution_id,
                  t.subset_task_execution_id,
                  t.parent_lu_name,
                  '' AS parent_entity_id,
                  '' AS parent_target_entity_id,
                  CASE WHEN t.lu_name = t.root_lu_name then t.root_entity_id else '' end as root_entity_id,
                  CASE WHEN t.lu_name = t.root_lu_name then t.root_target_entity_id else '' end as root_target_entity_id,
                  t.root_lu_name,
                  t.execution_note
              FROM
                  ${@TDMDB_SCEHMA}.task_execution_entities t
              WHERE
                  t.task_execution_id = ${task_execution_id}
          task_execution_id:
            link:
              path: PopulationArgs/parent_rows/TASK_EXECUTION_ID
              iterate: First
            schema: string
            mandatory: false
          TDMDB_SCEHMA:
            link: getTDMDBSchema/schema
            schema: string
            mandatory: false
        out:
          result:
            schema: '#ref'
  Insert All Into TASK_EXECUTION_LINK_ENTITIES:
    last: 1
    dependsOn: Select Query 1
    actors:
      Load All:
        parent: DbLoad
        in:
          interface:
            const: fabric
          schema:
            const: TDM
          table:
            const: TASK_EXECUTION_LINK_ENTITIES
          fields:
            const:
            - TASK_EXECUTION_ID
            - IID
            - LU_NAME
            - PARENT_LU_NAME
            - TARGET_ENTITY_ID
            - ENTITY_ID
            - PARENT_ENTITY_ID
            - BE_ROOT_ENTITY_ID
            - TARGET_ROOT_ENTITY_ID
            - EXECUTION_STATUS
            - TARGET_PARENT_ID
            - ROOT_LU_NAME
            - ROOT_ENTITY_STATUS
            - VERSION_TASK_EXECUTION_ID
            - SUBSET_TASK_EXECUTION_ID
            - ID_TYPE
            - EXECUTION_NOTE
          keys:
            const:
            - TASK_EXECUTION_ID
            - IID
            - LU_NAME
            - TARGET_ENTITY_ID
            - BE_ROOT_ENTITY_ID
            - TARGET_ROOT_ENTITY_ID
          TASK_EXECUTION_ID:
            link:
              path: Query All Records/result/task_execution_id
              iterate: Iterate
            schema: string
            mandatory: false
          IID:
            link:
              path: Query All Records/result/iid
              iterate: Iterate
            schema: string
            mandatory: false
          LU_NAME:
            link:
              path: Query All Records/result/lu_name
              iterate: Iterate
            schema: string
            mandatory: false
          PARENT_LU_NAME:
            link:
              path: Query All Records/result/parent_lu_name
              iterate: Iterate
            schema: string
            mandatory: false
          TARGET_ENTITY_ID:
            link:
              path: Query All Records/result/target_entity_id
              iterate: Iterate
            schema: string
            mandatory: false
          ENTITY_ID:
            link:
              path: Query All Records/result/entity_id
              iterate: Iterate
            schema: string
            mandatory: false
          PARENT_ENTITY_ID:
            link:
              path: Query All Records/result/parent_entity_id
              iterate: Iterate
            schema: string
            mandatory: false
          BE_ROOT_ENTITY_ID:
            link:
              path: Query All Records/result/root_entity_id
              iterate: Iterate
            schema: string
            mandatory: false
          TARGET_ROOT_ENTITY_ID:
            link:
              path: Query All Records/result/root_target_entity_id
              iterate: Iterate
            schema: string
            mandatory: false
          EXECUTION_STATUS:
            link:
              path: Query All Records/result/execution_status
              iterate: Iterate
            schema: string
            mandatory: false
          TARGET_PARENT_ID:
            link:
              path: Query All Records/result/parent_target_entity_id
              iterate: Iterate
            schema: string
            mandatory: false
          ROOT_LU_NAME:
            link:
              path: Query All Records/result/root_lu_name
              iterate: Iterate
            schema: string
            mandatory: false
          ROOT_ENTITY_STATUS:
            schema: string
            mandatory: false
          VERSION_TASK_EXECUTION_ID:
            link:
              path: Query All Records/result/version_task_execution_id
              iterate: Iterate
            schema: integer
            mandatory: false
          SUBSET_TASK_EXECUTION_ID:
            link:
              path: Query All Records/result/subset_task_execution_id
              iterate: Iterate
            schema: integer
            mandatory: false
          ID_TYPE:
            link:
              path: Query All Records/result/id_type
              iterate: Iterate
            schema: string
            mandatory: false
          EXECUTION_NOTE:
            link:
              path: Query All Records/result/execution_note
              iterate: Iterate
            schema: string
            mandatory: false
    split: '--------------------'
  'Insert Limited Entries Into TASK_EXECUTION_LINK_ENTITIES ':
    last: 1
    dependsOn: Select Query 2
    actors:
      Load Partial:
        parent: DbCommand
        in:
          interface:
            const: fabric
          sql:
            const: |-
              INSERT INTO
                  TDM.TASK_EXECUTION_LINK_ENTITIES(
                      TASK_EXECUTION_ID,
                      IID,
                      LU_NAME,
                      PARENT_LU_NAME,
                      TARGET_ENTITY_ID,
                      ENTITY_ID,
                      PARENT_ENTITY_ID,
                      BE_ROOT_ENTITY_ID,
                      TARGET_ROOT_ENTITY_ID,
                      EXECUTION_STATUS,
                      TARGET_PARENT_ID,
                      ROOT_LU_NAME,
                      ROOT_ENTITY_STATUS,
                      VERSION_TASK_EXECUTION_ID,
                      SUBSET_TASK_EXECUTION_ID,
                      ID_TYPE,
                      EXECUTION_NOTE)
                  values(
                      ${TASK_EXECUTION_ID},
                      ${IID},
                      ${LU_NAME},
                      ${PARENT_LU_NAME},
                      ${TARGET_ENTITY_ID},
                      ${ENTITY_ID},
                      ${PARENT_ENTITY_ID},
                      ${BE_ROOT_ENTITY_ID},
                      ${TARGET_ROOT_ENTITY_ID},
                      ${EXECUTION_STATUS},
                      ${TARGET_PARENT_ID},
                      ${ROOT_LU_NAME},
                      ${ROOT_ENTITY_STATUS},
                      ${VERSION_TASK_EXECUTION_ID},
                      ${SUBSET_TASK_EXECUTION_ID},
                      ${ID_TYPE},
                      ${EXECUTION_NOTE})
              on conflict do nothing
          TASK_EXECUTION_ID:
            link:
              path: Query With Limited Records/result/task_execution_id
              iterate: Iterate
            schema: integer
            mandatory: false
          ID_TYPE:
            link:
              path: Query With Limited Records/result/id_type
              iterate: Iterate
            schema: string
            mandatory: false
          SUBSET_TASK_EXECUTION_ID:
            link:
              path: Query With Limited Records/result/subset_task_execution_id
              iterate: Iterate
            schema: integer
            mandatory: false
          VERSION_TASK_EXECUTION_ID:
            link:
              path: Query With Limited Records/result/version_task_execution_id
              iterate: Iterate
            schema: integer
            mandatory: false
          ROOT_ENTITY_STATUS:
            schema: any
            mandatory: false
          ROOT_LU_NAME:
            link:
              path: Query With Limited Records/result/root_lu_name
              iterate: Iterate
            schema: string
            mandatory: false
          TARGET_PARENT_ID:
            link:
              path: Query With Limited Records/result/parent_target_entity_id
              iterate: Iterate
            schema: string
            mandatory: false
          EXECUTION_STATUS:
            link:
              path: Query With Limited Records/result/execution_status
              iterate: Iterate
            schema: string
            mandatory: false
          TARGET_ROOT_ENTITY_ID:
            link:
              path: Query With Limited Records/result/root_target_entity_id
              iterate: Iterate
            schema: string
            mandatory: false
          BE_ROOT_ENTITY_ID:
            link:
              path: Query With Limited Records/result/root_entity_id
              iterate: Iterate
            schema: string
            mandatory: false
          PARENT_ENTITY_ID:
            link:
              path: Query With Limited Records/result/parent_entity_id
              iterate: Iterate
            schema: string
            mandatory: false
          ENTITY_ID:
            link:
              path: Query With Limited Records/result/entity_id
              iterate: Iterate
            schema: string
            mandatory: false
          TARGET_ENTITY_ID:
            link:
              path: Query With Limited Records/result/target_entity_id
              iterate: Iterate
            schema: string
            mandatory: false
          PARENT_LU_NAME:
            link:
              path: Query With Limited Records/result/parent_lu_name
              iterate: Iterate
            schema: string
            mandatory: false
          LU_NAME:
            link:
              path: Query With Limited Records/result/lu_name
              iterate: Iterate
            schema: string
            mandatory: false
          IID:
            link:
              path: Query With Limited Records/result/iid
              iterate: Iterate
            schema: string
            mandatory: false
          EXECUTION_NOTE:
            link:
              path: Query With Limited Records/result/execution_note
              iterate: Iterate
            schema: any
            mandatory: false
  'Update Child Entity Status   ':
    dependsOn: Insert All Into TASK_EXECUTION_LINK_ENTITIES
    actors:
      Set Failed Roots:
        parent: DbCommand
        in:
          interface:
            const: fabric
          sql:
            const: "Update TASK_EXECUTION_LINK_ENTITIES set root_entity_status = 'failed'\n\
              \t    where ROOT_LU_NAME||TARGET_ROOT_ENTITY_ID in\n\t   (select ROOT_LU_NAME||TARGET_ROOT_ENTITY_ID\
              \ from TASK_EXECUTION_LINK_ENTITIES where Execution_Status <> 'completed');"
      Set Completed Roots:
        parent: DbCommand
        in:
          interface:
            const: fabric
          sql:
            const: Update TASK_EXECUTION_LINK_ENTITIES set root_entity_status = 'completed'
              where root_entity_status is null or root_entity_status='';
    split: '--------------------'
  Update Child Entity Status:
    dependsOn: 'Insert Limited Entries Into TASK_EXECUTION_LINK_ENTITIES '
    actors:
      DbCommand Child Entites1:
        parent: DbCommand
        in:
          interface:
            const: fabric
          sql:
            const: |
              UPDATE TASK_EXECUTION_LINK_ENTITIES
              SET root_entity_status =
                  CASE
                      WHEN execution_status = 'failed' THEN 'failed'
                      ELSE 'completed'
                  END
              WHERE execution_status IS NOT NULL;
  Do Nothing:
    dependsOn: 'Update Child Entity Status   '
    split: '--------------------'
  Get Failed Roots:
    dependsOn: Update Child Entity Status
    actors:
      'DbCommand Root Status ':
        parent: DbCommand
        in:
          interface:
            const: TDM
          sql:
            const: |-
              WITH root_list AS (SELECT DISTINCT
                      root_entity_id,
                      root_lu_name,
                      root_target_entity_id
                  FROM ${@TDMDB_SCHEMA}.task_execution_entities
                  WHERE task_execution_id = ${task_execution_id} AND  execution_status <> 'completed')
              SELECT DISTINCT t1.lu_name,t1.entity_id,t1.target_entity_id, t1.iid
              FROM ${@TDMDB_SCHEMA}.task_execution_entities t1
              JOIN root_list rl
              ON t1.root_entity_id = rl.root_entity_id AND t1.root_lu_name = rl.root_lu_name And t1.root_target_entity_id = rl.root_target_entity_id
              WHERE t1.task_execution_id = ${task_execution_id};
          TDMDB_SCHEMA:
            link: getTDMDBSchema/schema
            schema: string
            mandatory: false
          task_execution_id:
            link:
              path: PopulationArgs/parent_rows/TASK_EXECUTION_ID
              iterate: First
            schema: string
            mandatory: false
        out:
          result:
            schema: '#ref'
  'Do Nothing ':
    dependsOn: Do Nothing
    split: '--------------------'
  Failed Hierarchy:
    dependsOn: Get Failed Roots
    actors:
      DbCommand Failed Roots Partial Hierarchy:
        parent: DbCommand
        in:
          interface:
            const: fabric
          sql:
            const: "Update TASK_EXECUTION_LINK_ENTITIES set root_entity_status = 'failed'\n\
              \t    where TASK_EXECUTION_ID = ${TASK_EXECUTION_ID} AND LU_NAME = ${LU_NAME}\
              \ AND TARGET_ENTITY_ID =${TARGET_ENTITY_ID}\n\t\tAND ENTITY_ID=${ENTITY_ID}\
              \ AND IID = ${IID}"
          batch:
            const: true
          LU_NAME:
            link:
              path: DbCommand Root Status /result/lu_name
              iterate: Iterate
            schema: string
            mandatory: false
          TARGET_ENTITY_ID:
            link:
              path: DbCommand Root Status /result/target_entity_id
              iterate: Iterate
            schema: string
            mandatory: false
          ENTITY_ID:
            link:
              path: DbCommand Root Status /result/entity_id
              iterate: Iterate
            schema: string
            mandatory: false
          TASK_EXECUTION_ID:
            link:
              path: PopulationArgs/parent_rows/TASK_EXECUTION_ID
              iterate: First
            schema: string
            mandatory: false
          IID:
            link:
              path: DbCommand Root Status /result/iid
              iterate: Iterate
            schema: string
            mandatory: false
schemas:
  PopulationArgs.out.parent_rows:
    type: array
    items:
      type: object
      properties:
        TASK_EXECUTION_ID:
          type: string
  Query All Records.in.task_execution_id:
    type: array
    items:
      type: string
  Query All Records.out.result:
    type: array
    items:
      type: object
      properties:
        task_execution_id:
          type: integer
        lu_name:
          type: string
        entity_id:
          type: string
        target_entity_id:
          type: string
        execution_status:
          type: string
        iid:
          type: string
        id_type:
          type: string
        version_task_execution_id:
          type: integer
        subset_task_execution_id:
          type: integer
        parent_lu_name:
          type: string
        parent_entity_id:
          type: string
        parent_target_entity_id:
          type: string
        root_entity_id:
          type: string
        root_target_entity_id:
          type: string
        root_lu_name:
          type: string
        execution_note:
          type: string
  Query With Limited Records.out.result:
    type: array
    items:
      type: object
      properties:
        task_execution_id:
          type: integer
        lu_name:
          type: string
        entity_id:
          type: string
        target_entity_id:
          type: string
        execution_status:
          type: string
        iid:
          type: string
        id_type:
          type: string
        version_task_execution_id:
          type: integer
        subset_task_execution_id:
          type: integer
        parent_lu_name:
          type: string
        parent_entity_id:
          type: string
        parent_target_entity_id:
          type: string
        root_entity_id:
          type: string
        root_target_entity_id:
          type: string
        root_lu_name:
          type: string
        execution_note:
          type: string
  DbCommand Root Status .out.result:
    type: array
    items:
      type: object
      properties:
        lu_name:
          type: string
        entity_id:
          type: string
        target_entity_id:
          type: string
        iid:
          type: string
