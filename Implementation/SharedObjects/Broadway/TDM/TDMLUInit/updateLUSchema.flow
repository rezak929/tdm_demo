                                      "column": tableObj.column_name,
                                      "columnId": tableObj.id
                                    }
                                    },
                                    "To": {
                                    "_attributes": {
                                      "tableType": "TableObject",
                                      "table": "TDM_BE_IIDS",
                                      "populationObjectName": "populationTdmBeIID.flow",
                                      "column": "iid",
                                      "columnId": "iid"
                                    }
                                    }
                                  };
                        arrIn.push({"_value":objTmp});

                    }

                }
                objOut.DatabaseRelations = {"_value" : { "Relation": arrIn} };
                objOut;
