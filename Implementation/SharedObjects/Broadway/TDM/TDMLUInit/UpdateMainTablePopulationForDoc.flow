tags: TDM
stages:
  PWD:
    actors:
      Get Project Directory:
        parent: InnerFlow
        disabled: true
        in:
          flowName:
            const: GetProjectFolder
        out:
          PROJECT_FOLDER:
            schema: string
      PROJECT_DIR:
        parent: SysEnv
        in:
          name:
            const: PROJECT_DIR
        out:
          value:
            schema: string
  Set Folder And Flow Names:
    actors:
      LU Folder:
        parent: StringFormat
        in:
          format:
            const: ${projDir}/Implementation/LogicalUnits/${LU_NAME}/Tables/${TABLE_NAME}.population.flow
          projDir:
            link:
            - Get Project Directory/PROJECT_FOLDER
            - PROJECT_DIR/value
            schema: string
            mandatory: false
          LU_NAME:
            external: LU_NAME
            schema: string
            mandatory: false
          TABLE_NAME:
            external: TABLE_NAME
            schema: string
            mandatory: false
  Add Distribution of const 1:
    actors:
      LuFunction1:
        parent: LuFunction
        in:
          functionName:
            const: replaceDistribution
          fileName:
            link: LU Folder/string
            schema: string
            mandatory: false
