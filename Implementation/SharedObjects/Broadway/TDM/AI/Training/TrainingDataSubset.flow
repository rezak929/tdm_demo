tags: TDM
stages:
  Get Inputs:
    actors:
      luName:
        parent: Const
        in:
          value:
            const: null
            external: luName
        out:
          value:
            schema: string
      dcName:
        parent: Const
        in:
          value:
            const: null
            external: dcName
        out:
          value:
            schema: string
      taskExecutionID:
        parent: Const
        in:
          value:
            const: null
            external: taskExecutionID
        out:
          value:
            schema: string
      LuID:
        parent: Const
        in:
          value:
            const: null
            external: LuID
            schema: string
        out:
          value:
            schema: string
      Now:
        parent: Now
      Set LuType:
        parent: FabricSet
        in:
          key:
            const: LU_TYPE
          value:
            const: null
            external: luName
            default: false
  Set Inputs:
    actors:
      enable_masking:
        parent: FabricSet
        in:
          key:
            default: false
          value:
            default: false
      postgres schema name:
        parent: Lowercase
        in:
          string:
            link: luName/value
      getTDMDBSchema:
        parent: InnerFlow
        in:
          flowName:
            const: getTDMDBSchema
        out:
          schema:
            schema: string
      'Format the current date to load it ':
        parent: DateFormat
        in:
          date:
            link: Now/date
      AI_DB_K2SYSTEM1:
        parent: FabricSetRead
        in:
          key:
            const: AI_DB_INTERFACE
      TDM_TASK_EXE_ID:
        parent: FabricSet
        in:
          key:
            const: TDM_TASK_EXE_ID
            default: false
          value:
            link: taskExecutionID/value
      Set TDM_LU_NAME:
        parent: FabricSet
        in:
          key:
            const: TDM_LU_NAME
            default: false
          value:
            link: luName/value
      AI_ENVIRONMENT:
        parent: FabricSetRead
        in:
          key:
            const: AI_ENVIRONMENT
            default: false
  'Get Execution Parameters ':
    actors:
      set environment:
        parent: FabricSet
        in:
          key:
            const: environment
            default: false
          value:
            const: null
            link: AI_ENVIRONMENT/result
            default: false
      StringFormat:
        parent: StringFormat
        in:
          format:
            const: ${interfcae}_External
          interfcae:
            link: AI_DB_K2SYSTEM1/result
            schema: string
            mandatory: false
  Prepare Infra:
    actors:
      getNamespaceName1:
        parent: InnerFlow
        in:
          flowName:
            const: getNamespaceName
          taskExecutionID:
            link: taskExecutionID/value
            schema: any
            mandatory: false
        out:
          string:
            schema: string
      Get k2system Schema Name:
        parent: InnerFlow
        in:
          flowName:
            const: Getk2SystemSchemaName
        out:
          result:
            schema: string
      'Get Interface Details ':
        parent: LuFunction
        in:
          functionName:
            const: fnValidateInterfaceDetails
          Interface:
            link: StringFormat/string
            schema: string
            mandatory: false
        out:
          result:
            schema: '#ref'
  Init Infra:
    actors:
      'ErrorHandler 3   ':
        parent: ErrorHandler
        error: result
        in:
          config:
            const:
            - exceptionKey: java.lang.Exception
              conditions:
                message: ''
              actions:
                suppress: false
                log: true
                flowName: PopulateAITablesWithFailed
      kubeBuildMedoidInfraInit1:
        parent: InnerFlow
        in:
          flowName:
            const: kubeBuildMedoidInfraInit
          namespace:
            link: getNamespaceName1/string
            schema: string
            mandatory: false
          pg-pass:
            link: Get Interface Details /result/Password
            schema: string
            mandatory: false
          pg-host:
            link: Get Interface Details /result/Host
            schema: string
            mandatory: false
          pg-db:
            link: Get Interface Details /result/Data
            schema: string
            mandatory: false
          pg-port:
            link: Get Interface Details /result/Port
            schema: string
            mandatory: false
          pg-user:
            link: Get Interface Details /result/User
            schema: string
            mandatory: false
  'Execute Infra ':
    actors:
      New Task task_executions:
        parent: DbCommand
        in:
          interface:
            const: null
            link: AI_DB_K2SYSTEM1/result
          sql:
            const: UPDATE ${@k2systemName}.task_executions SET status='NEW',last_update_time=NOW()
              where id=${task_execution_id}
          task_execution_id:
            link: taskExecutionID/value
            schema: string
            mandatory: false
          k2systemName:
            link: Get k2system Schema Name/result
            schema: string
            mandatory: false
      wait 10 Sec:
        parent: Sleep
        in:
          millis:
            const: 10000
      kubeCreateTrainingTaskGPU1:
        parent: InnerFlow
        in:
          flowName:
            const: kubeCreateTrainingTaskGPU
          namespace:
            link: getNamespaceName1/string
            schema: string
            mandatory: false
          id:
            link: taskExecutionID/value
            schema: string
            mandatory: false
  'Update task execution list ':
    actors:
      task_execution_list:
        parent: DbCommand
        in:
          interface:
            const: TDM
          sql:
            const: "UPDATE ${@TDMDB_SCHEMA}.task_execution_list\r\n\tSET   execution_status='running'\r\
              \n\tWHERE task_execution_id=${EXEID} and lu_id=${lu_id} and process_id=-1;"
          TDMDB_SCHEMA:
            link: getTDMDBSchema/schema
            schema: string
            mandatory: false
          EXEID:
            link: taskExecutionID/value
            schema: string
            mandatory: false
          lu_id:
            link: LuID/value
            schema: string
            mandatory: false
  Check Training Task Status:
    actors:
      ErrorHandler 4:
        parent: ErrorHandler
        error: result
        in:
          config:
            const:
            - exceptionKey: java.lang.Exception
              conditions:
                message: ''
              actions:
                suppress: false
                log: true
                flowName: PopulateAITablesWithFailed
      Check task status:
        parent: LuFunction
        in:
          functionName:
            const: fnCheckMedoidTaskStatus
          interface_name:
            link: AI_DB_K2SYSTEM1/result
            schema: string
            mandatory: false
          task_id:
            link: taskExecutionID/value
            schema: string
            mandatory: false
          task_type:
            const: TRAINING
            schema: string
            mandatory: false
        out:
          taskStatus:
            schema: string
  If Training Job Failed:
    dependsOn: Check Training Task Status
    actors:
      EqualsIgnoreCase1:
        parent: EqualsIgnoreCase
        condition: result
        in:
          b:
            const: FAILED
            schema: string
          a:
            link: Check task status/taskStatus
    split: '--------------------'
  Do Nothing:
    else: true
    transactional: false
    dependsOn: Check Training Task Status
  'Fail  ':
    dependsOn: If Training Job Failed
    actors:
      PopulateAITablesWithFailed:
        parent: InnerFlow
        in:
          flowName:
            const: PopulateAITablesWithFailed
          error:
            schema: any
            mandatory: false
      Throw Exception:
        parent: JavaScript
        in:
          script:
            const: throw "The AI Task Failed"
    split: '--------------------'
  'Do Nothing   ':
    transactional: false
    dependsOn: Do Nothing
schemas:
  Get Interface Details .out.result:
    type: object
    properties:
      Data:
        type: string
      Host:
        type: string
      ioprovider:
        type: string
      Password:
        type: string
      Port:
        type: string
      TechType:
        type: string
      User:
        type: string
