tags: Medoid,TDM
stages:
  Create Namespace:
    actors:
      kubeCreateNamespace:
        parent: InnerFlow
        in:
          flowName:
            const: kubeCreateNamespace
          namespace:
            external: namespace
            schema: string
            mandatory: false
  Create Secret:
    actors:
      kubeCreateSecret:
        parent: InnerFlow
        in:
          flowName:
            const: kubeCreateSecret
          namespace:
            external: namespace
            schema: string
            mandatory: false
          pg-pass:
            external: pg-pass
            schema: string
            mandatory: false
          pg-host:
            external: pg-host
            schema: string
            mandatory: false
          pg-db:
            external: pg-db
            schema: string
            mandatory: false
          pg-port:
            external: pg-port
            schema: string
            mandatory: false
          pg-user:
            external: pg-user
            schema: string
            mandatory: false
  Create PVC:
    actors:
      kubeCreatePVC:
        parent: InnerFlow
        in:
          flowName:
            const: kubeCreatePVC
          namespace:
            external: namespace
            schema: string
            mandatory: false
