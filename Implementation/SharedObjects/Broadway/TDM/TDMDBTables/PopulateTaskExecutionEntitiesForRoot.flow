tags: TDM
stages:
  Prepare the Data:
    actors:
      Get Entity Status:
        parent: FabricSetRead
        in:
          key:
            const: ENTITY_STATUS
            default: false
      Get Source Entity ID:
        parent: FabricSetRead
        in:
          key:
            const: SOURCE_ENTITY_ID
            default: false
      Get LU Type:
        parent: FabricSetRead
        in:
          key:
            const: LU_TYPE
            default: false
      Get IID:
        parent: FabricSetRead
        in:
          key:
            const: IID
            default: false
      Get Task Execution ID:
        parent: FabricSetRead
        in:
          key:
            const: TDM_TASK_EXE_ID
            default: false
      Get Target Entity ID:
        parent: FabricSetRead
        height: 99
        in:
          key:
            const: TARGET_ENTITY_ID
            default: false
  Continue Preparing:
    actors:
      Get Timing of Processing Start:
        parent: FabricSetRead
        in:
          key:
            const: IID_START_DATETIME
            default: false
      Get Source Environment:
        parent: FabricSetRead
        in:
          key:
            const: TDM_SOURCE_ENVIRONMENT_NAME
            default: false
      Get Timing of Processing End:
        parent: FabricSetRead
        in:
          key:
            const: IID_AFTER_GET_DATETIME
            default: false
      Now:
        parent: Now
      Split UID:
        parent: LuFunction
        in:
          functionName:
            const: fnSplitUID2
          uid:
            link: Get Source Entity ID/result
            schema: string
            mandatory: false
        out:
          instanceID:
            schema: string
          envName:
            schema: string
          versionExeID:
            schema: string
      Check if Target Entity ID is Not Set:
        parent: Equals
        height: 70
        in:
          a:
            link: Get Target Entity ID/result
            schema: string
          b:
            const: NO_ID
            schema: string
  Check env:
    actors:
      Synthetic:
        parent: EqualsIgnoreCase
        in:
          b:
            const: Synthetic
            schema: string
          a:
            link: Get Source Environment/result
      AI:
        parent: EqualsIgnoreCase
        in:
          b:
            const: AI
            schema: string
          a:
            link: Get Source Environment/result
  'Else ':
    else: true
    transactional: false
    dependsOn: Check env
    split: '--------------------'
  Check if Generate Rule based:
    transactional: false
    dependsOn: Check env
    actors:
      Or:
        parent: Or
        condition: result
        in:
          a:
            link: Synthetic/result
          b:
            link: AI/result
  Check For Version:
    transactional: false
    dependsOn: 'Else '
    actors:
      Verison execution id is empty:
        parent: EqualsIgnoreCase
        condition: result
        in:
          b:
            const: ''
            schema: string
          a:
            link: Split UID/versionExeID
      Default value:
        parent: Const
        in:
          value:
            const: 0
            schema: integer
        out:
          value:
            schema: integer
    split: '--------------------'
  'versionExeID   ':
    else: true
    transactional: false
    dependsOn: 'Else '
    actors:
      versionExeID:
        parent: Const
        in:
          value:
            const: null
            link: Split UID/versionExeID
        out:
          value:
            schema: string
    split: '--------------------'
  'Continue ':
    transactional: false
    dependsOn: Check if Generate Rule based
  Continue:
    dependsOn:
    - 'versionExeID   '
    - Check For Version
    split: '--------------------'
  Check for Subset:
    transactional: false
    dependsOn: 'Continue '
    actors:
      Subset execution id is empty:
        parent: EqualsIgnoreCase
        condition: result
        in:
          b:
            const: ''
            schema: string
          a:
            link: Split UID/versionExeID
      'Default value  ':
        parent: Const
        in:
          value:
            const: 0
            schema: integer
        out:
          value:
            schema: integer
    split: '--------------------'
  'subsetExeID  ':
    else: true
    transactional: false
    dependsOn: 'Continue '
    actors:
      subsetExeID:
        parent: Const
        in:
          value:
            const: null
            link: Split UID/versionExeID
        out:
          value:
            schema: string
  Calculate Fabric Get Duration and Total Duration:
    actors:
      Get Clone No:
        parent: FabricSetRead
        in:
          key:
            const: clone_id
            default: false
      Format the Current Time:
        parent: DateFormat
        in:
          format:
            const: yyyy-MMM-dd HH:mm:ss.SSS
          date:
            link: Now/date
      getTDMDBSchema:
        parent: InnerFlow
        in:
          flowName:
            const: getTDMDBSchema
        out:
          schema:
            schema: string
      Calculate Total Processing Duration:
        parent: DateDuration
        in:
          from:
            link: Get Timing of Processing Start/result
          to:
            link: Now/date
      If Target Entity ID is not set, use source ID:
        parent: IfElse
        in:
          a:
            link: Get IID/result
            schema: string
          b:
            link: Get Target Entity ID/result
            schema: string
          test:
            link: Check if Target Entity ID is Not Set/result
        out:
          result:
            schema: string
      Calculate Total Get Duration:
        parent: DateDuration
        in:
          from:
            link: Get Timing of Processing Start/result
          to:
            link: Get Timing of Processing End/result
  Load Data to Entities table:
    actors:
      Add Entity to Entities Table:
        parent: DbCommand
        in:
          interface:
            const: TDM
          sql:
            const: |-
              INSERT INTO ${@schema}.TASK_EXECUTION_ENTITIES(TASK_EXECUTION_ID, LU_NAME, ENTITY_ID, TARGET_ENTITY_ID, ENV_ID, EXECUTION_STATUS, ID_TYPE,
              IID, SOURCE_ENV, CREATION_DATE, ENTITY_START_TIME, ENTITY_END_TIME, FABRIC_GET_TIME,
              TOTAL_PROCESSING_TIME, CLONE_NO, ROOT_ENTITY_ID, ROOT_LU_NAME, ROOT_TARGET_ENTITY_ID, PARENT_LU_NAME,VERSION_TASK_EXECUTION_ID,SUBSET_TASK_EXECUTION_ID)
              VALUES (${taskexecutionId}, ${luName}, ${sourceEntityId}, ${targetEntityId}, ${targetEnvId}, ${executionStatus}, ${idType},
               ${iid},${sourceEnv},Now(), ${entityStartTime}, ${entityEndTime}, ${fabticTime},
              ${totalTime}, ${cloneNo}, ${rootEntityId}, ${rootLUName}, ${rootTargetEntityId}, '',${versionTaskExeID},${subsetTaskExeID})
              ON CONFLICT ON CONSTRAINT task_execution_entities_pkey Do update set execution_status = ${executionStatus}
          luName:
            link: Get LU Type/result
            schema: string
          taskexecutionId:
            link: Get Task Execution ID/result
            schema: string
          sourceEntityId:
            link: Get Source Entity ID/result
            schema: string
          targetEntityId:
            link: If Target Entity ID is not set, use source ID/result
            schema: string
          executionStatus:
            link: Get Entity Status/result
            schema: string
          iid:
            link: Get IID/result
            schema: string
          entityStartTime:
            link: Get Timing of Processing Start/result
            schema: string
          entityEndTime:
            link: Format the Current Time/string
            schema: string
          fabticTime:
            link: Calculate Total Get Duration/millis
            schema: number
          totalTime:
            link: Calculate Total Processing Duration/millis
            schema: number
          idType:
            const: ENTITY
            schema: string
          cloneNo:
            link: Get Clone No/result
            schema: string
          schema:
            link: getTDMDBSchema/schema
            schema: string
            mandatory: false
          rootEntityId:
            link: Get IID/result
            schema: string
            mandatory: false
          rootLUName:
            link: Get LU Type/result
            schema: string
            mandatory: false
          rootTargetEntityId:
            link: If Target Entity ID is not set, use source ID/result
            schema: string
            mandatory: false
          targetEnvId:
            external: targetEnvId
            schema: string
            mandatory: false
          sourceEnv:
            external: sourceEnv
            schema: string
            mandatory: false
          versionTaskExeID:
            link:
            - Default value/value
            - versionExeID/value
            schema: any
            mandatory: false
          subsetTaskExeID:
            link:
            - Default value  /value
            - subsetExeID/value
            schema: any
            mandatory: false
