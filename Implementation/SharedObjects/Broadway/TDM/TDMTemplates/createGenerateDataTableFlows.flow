tags: TDM
stages:
  Get List of Tables:
    actors:
      Get List Of Tables:
        parent: LuFunction
        in:
          functionName:
            const: getTablesForGenerate
          luName:
            external: LU_NAME
            schema: string
            mandatory: false
          sourceInterface:
            external: SOURCE_INTERFACE
            schema: string
            editor:
              id: com.k2view.interface
              interfaceType:
              - database
            mandatory: false
          sourceSchema:
            external: SOURCE_SCHEMA
            schema: string
            mandatory: false
          useFabric:
            const: ''
            external: useFabric
            schema: boolean
            default: true
            mandatory: false
        out:
          res:
            schema: '#ref'
      Create Folder:
        parent: InnerFlow
        in:
          flowName:
            const: createFolderForFlows
          LU_NAME:
            external: LU_NAME
            schema: string
            editor:
              id: com.k2view.logicalUnit
              addEmptyEntry: true
            mandatory: false
          FOLDER_NAME:
            const: GeneratorFlows
            schema: string
            mandatory: false
  Create the Name Of the Flow:
    actors:
      create Generator Flow Name:
        parent: InnerFlow
        height: 284
        in:
          flowName:
            const: createGeneratorFlowName
          luName:
            external: LU_NAME
            schema: string
            editor:
              id: com.k2view.logicalUnit
              addEmptyEntry: true
            mandatory: false
          tableName:
            link:
              path: Get List Of Tables/res/luTable
              iterate: Iterate
            schema: string
            mandatory: false
        out:
          generatorFlowNames:
            schema: '#ref'
  Check If Flow Already Exists:
    dependsOn: Create the Name Of the Flow
    actors:
      Override Existing Flow?:
        parent: Equals
        condition: result
        in:
          a:
            external: OVERRIDE_EXISTING_FLOWS
            schema: boolean
          b:
            const: false
            schema: boolean
      Check if Flow Already Exists:
        parent: InnerFlow
        in:
          flowName:
            const: CheckIfFlowExists
          LU_NAME:
            external: LU_NAME
            schema: string
            mandatory: false
          FOLDER_NAME:
            const: GeneratorFlows
            schema: string
            mandatory: false
          FLOW_FILE_NAME:
            link:
              path: create Generator Flow Name/generatorFlowNames
              iterate: Iterate
            schema: string
            mandatory: false
        out:
          SKIP_TABLE:
            schema: boolean
    split: '--------------------'
  Create Flow Always:
    else: true
    transactional: false
    dependsOn: Create the Name Of the Flow
    actors:
      create Flow:
        parent: InnerFlow
        in:
          flowName:
            const: createGenerateDataPopulations
          luType:
            external: LU_NAME
            schema: string
            editor:
              id: com.k2view.logicalUnit
          table:
            link:
              path: Get List Of Tables/res/luTable
              iterate: Iterate
            schema: string
            mandatory: false
          generatorFlowName:
            link:
              path: create Generator Flow Name/generatorFlowNames
              iterate: Iterate
            schema: string
            mandatory: false
          sourceInterface:
            external: SOURCE_INTERFACE
            schema: string
            editor:
              id: com.k2view.mTableKey
            mandatory: false
          sourceSchema:
            external: SOURCE_SCHEMA
            schema: string
            editor:
              id: com.k2view.mTableKey
            mandatory: false
          sourceTable:
            link:
              path: Get List Of Tables/res/sourceTable
              iterate: Iterate
            schema: string
            editor:
              id: com.k2view.mTableKey
            mandatory: false
          params:
            schema: '#ref'
            editor:
              id: com.k2view.strings
            mandatory: false
          DOCUMENT_IND:
            external: DOCUMENT_IND
            schema: boolean
            mandatory: false
          mainTableName:
            external: ROOT_TABLE_NAME
            schema: string
            editor:
              id: com.k2view.mTableKey
            mandatory: false
          INTERFACE_TYPE:
            external: INTERFACE_TYPE
            schema: string
            mandatory: false
  Create New Flow:
    last: 1
    dependsOn: Check If Flow Already Exists
    actors:
      If flow Does Not Exists:
        parent: Equals
        condition: result
        in:
          a:
            link: Check if Flow Already Exists/SKIP_TABLE
            schema: boolean
          b:
            const: false
            schema: boolean
      create Flow for New Table:
        parent: InnerFlow
        in:
          flowName:
            const: createGenerateDataPopulations
          luType:
            external: LU_NAME
            schema: string
            editor:
              id: com.k2view.logicalUnit
          table:
            link:
              path: Get List Of Tables/res/luTable
              iterate: Iterate
            schema: string
            mandatory: false
          generatorFlowName:
            link:
              path: create Generator Flow Name/generatorFlowNames
              iterate: Iterate
            schema: string
            mandatory: false
          sourceInterface:
            external: SOURCE_INTERFACE
            schema: string
            editor:
              id: com.k2view.mTableKey
            mandatory: false
          sourceSchema:
            external: SOURCE_SCHEMA
            schema: string
            editor:
              id: com.k2view.mTableKey
            mandatory: false
          sourceTable:
            link:
              path: Get List Of Tables/res/sourceTable
              iterate: Iterate
            schema: string
            editor:
              id: com.k2view.mTableKey
            mandatory: false
          params:
            schema: '#ref'
            editor:
              id: com.k2view.strings
            mandatory: false
          DOCUMENT_IND:
            external: DOCUMENT_IND
            schema: boolean
            mandatory: false
          mainTableName:
            external: ROOT_TABLE_NAME
            schema: string
            editor:
              id: com.k2view.mTableKey
            mandatory: false
          INTERFACE_TYPE:
            external: INTERFACE_TYPE
            schema: string
            mandatory: false
    split: '--------------------'
  Flow Already Exists:
    last: 1
    else: true
    transactional: false
    dependsOn: Check If Flow Already Exists
    actors:
      Report to Log:
        parent: Logger
        in:
          message:
            const: 'The LOAD Flow for table: <${TABLE_NAME}> Exists and it will not
              recreated. If you want to recreate it please delete its related flow'
          level:
            const: info
          TABLE_NAME:
            link:
              path: Get List Of Tables/res/luTable
              iterate: Iterate
            schema: string
    split: '--------------------'
  Stage 6:
    last: 1
    else: false
    transactional: false
    dependsOn: Create Flow Always
schemas:
  Get List Of Tables.out.res:
    type: array
    items:
      type: object
      properties:
        luTable:
          type: string
        sourceTable:
          type: string
  create Generator Flow Name.out.generatorFlowNames:
    type: array
    items:
      type: string
  create Flow.in.params:
    type: array
    items:
      type: string
  create Flow for New Table.in.params:
    type: array
    items:
      type: string
