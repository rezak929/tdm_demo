        PARENT_TABLE:
          type: string
        PARENT_FIELD_NAME:
          type: string
        FIELD_NAME:
          type: string
  Const1.out.value:
    type: array
    items:
      type: object
      properties:
        datatype:
          type: string
        name:
          type: string
        upperName:
          type: string
  JavaScript1.in.array1:
    type: array
    items:
      type: object
      properties:
        datatype:
          type: string
