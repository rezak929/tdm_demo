          type: string
  JavaScript1.in.array2:
    type: array
    items:
      type: object
      properties:
        datatype:
          type: string
        name:
          type: string
  JavaScript1.out.result:
    type: array
    items:
      type: object
      properties:
        datatype:
          type: string
        name:
          type: string
